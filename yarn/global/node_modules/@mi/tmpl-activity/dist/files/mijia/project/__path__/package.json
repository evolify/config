    "goku-production": "miot-goku xbox --build-command='npm run build-production' --archive-git-url git@git.n.xiaomi.com:iot-web/webapp-archive.git"
  },
  "dependencies": {
    "@mi/fit-ui": "^0.1.1",
    "@mi/miot-track": "^0.2.1",
    "@mi/smarthome-jssdk": "^0.1.0",
    "axios": "^0.18.0",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.10.4",
    "prop-types": "^15.7.2",
    "react-content-loader": "^4.3.2",
    "react-lazyload": "^2.6.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@mi/miot-goku": "^1.0.2",
    "@mi/miot-react-scripts": "^2.0.3",
    "babel-plugin-react-directive": "^0.1.1"
