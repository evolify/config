{
  "name": "@mi/tmpl-react",
  "version": "0.1.9",
  "description": "React related schematics",
  "scripts": {
    "build:ts": "tsc -p tsconfig.json",
    "copy:schemas": "cp src/schema.json dist/",
    "copy:files": "cp -r src/files dist/files",
    "copy:collection": "cp src/collection.json dist/collection.json",
    "build": "npm run build:ts && npm run copy:schemas && npm run copy:files && npm run copy:collection",
    "test": "npm run build && jasmine src/**/*_spec.js"
  },
  "keywords": [
    "schematics"
  ],
  "author": "",
  "license": "MIT",
  "schematics": "./dist/collection.json",
  "dependencies": {
    "@angular-devkit/core": "^9.1.1",
    "@angular-devkit/schematics": "^9.1.1",
    "typescript": "~3.8.2"
  },
  "devDependencies": {
    "@types/jasmine": "~3.5.0",
    "@types/node": "^12.11.1",
    "jasmine": "^3.5.0"
  },
  "files": [
    "dist"
  ],
  "gitHead": "30b7a5195f3bb041792ef1daadba45d72bbcd3b6"
}
